<?xml version="1.0"?>
<extension key="ca.bidon.formautosave" type="module">
  <file>formautosave</file>
  <name>CiviCRM form auto-save</name>
  <description>Automatically saves CiviCRM forms to avoid losing data when unexpected errors occur.</description>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <license>AGPL3+</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2018-01-26</releaseDate>
  <version>1.2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Support available from https://www.symbiotic.coop/en</comments>
  <urls>
    <url desc="Main Extension Page">https://github.com/mlutfy/formautosave</url>
    <url desc="Documentation">https://github.com/mlutfy/formautosave/blob/master/README.md</url>
    <url desc="Support">https://www.symbiotic.coop/en</url>
    <url desc="Licensing">https://civicrm.org/licensing</url>
  </urls>
  <civix>
    <namespace>CRM/Formautosave</namespace>
  </civix>
</extension>
